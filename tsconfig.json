{
  "extends": "@tsconfig/node20/tsconfig.json",
  "compilerOptions": {
    "types": ["node"],
    "checkJs": false,
    "target": "ESNext",
    "module": "ES2015",
    "moduleResolution": "Bundler",
    "esModuleInterop": true,
    "verbatimModuleSyntax": true,
    "noUncheckedIndexedAccess": true,
    "typeRoots": [
      ".",
    ]
  },
}
